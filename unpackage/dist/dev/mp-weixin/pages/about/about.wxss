page {
  min-height: 100%;
}
.about {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  text-align: center;
  padding: 40rpx;
  height: 100%;
}
.about .content-box {
  position: fixed;
  width: 262rpx;
  margin: 0 auto;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -70%);
      -ms-transform: translate(-50%, -70%);
          transform: translate(-50%, -70%);
}
.about .content-box .logo-pic {
  display: block;
  width: 262rpx;
  height: 262rpx;
}
.about .content-box .app-name {
  display: block;
  font-size: 36rpx;
  color: #393E4C;
  font-weight: bold;
  margin-top: 20rpx;
}
.about .content-box .app-version {
  display: block;
  font-size: 28rpx;
  color: #D1D3DE;
}

