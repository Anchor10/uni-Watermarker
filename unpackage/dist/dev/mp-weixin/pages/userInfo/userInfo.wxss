.user-info {
  display: block;
}
.user-info .change-avatar {
  display: block;
  width: 200rpx;
  margin: 40rpx auto 0;
}
.user-info .change-avatar .avatar-url {
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin: 0 auto;
}
.user-info .change-avatar .avatar-btn {
  display: block;
  text-align: center;
  font-size: 28rpx;
  color: #1F1F1F;
  margin-top: 20rpx;
  font-weight: bold;
}
.center-list {
  background-color: #FFFFFF;
  margin-top: 64rpx;
  width: 750rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  border-top: 1rpx solid #eee;
}
.center-list-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  min-height: 90rpx;
  width: 750rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
      -ms-flex-direction: row;
          flex-direction: row;
  padding: 0rpx 32rpx;
  border-bottom-width: 1rpx;
  border-color: #EEEEEE;
  border-bottom-style: solid;
}
.list-title {
  min-height: 90rpx;
  line-height: 90rpx;
  color: #1F1F1F;
  text-align: left;
  font-size: 28rpx;
  width: 120rpx;
}
.list-text {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  min-height: 90rpx;
  padding: 25rpx 0;
  color: #666;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  text-align: left;
  font-size: 28rpx;
  padding-right: 20rpx;
}
.line-height-sm {
  line-height: 1.5;
}
/*右箭头*/
.icon2 {
  display: block;
  height: 90rpx;
  width: 40rpx;
  line-height: 90rpx;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 32rpx 32rpx;
  color: #555;
  text-align: right;
}

