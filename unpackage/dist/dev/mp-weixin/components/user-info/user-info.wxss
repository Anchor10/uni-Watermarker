      -ms-flex-align: left;
          align-items: left;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  margin-left: 20rpx;
}
.logo-title .user-name {
  line-height: 44rpx;
  font-size: 38rpx;
  color: #1f1F1f;
  font-size: 28rpx;
  font-weight: bold;
}
.logo-title .user-name .vip-icon {
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
  margin-left: 20rpx;
}
.logo-title .score {
  font-size: 28rpx;
  color: #1F1F1F;
  line-height: 44rpx;
}
.logo-title .score text {
  margin-left: 10rpx;
}
.logo-title .vip-date {
  font-size: 28rpx;
  color: #1F1F1F;
  line-height: 44rpx;
}
.logo-title .vip-date text {
  margin-left: 10rpx;
