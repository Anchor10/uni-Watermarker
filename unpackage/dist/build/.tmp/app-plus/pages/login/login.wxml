<view class="login" bind:__l="__l"><view class="phone-num input-box"><input class="input-text" type="text" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['$0','phoneNum','$event',['trim']],['userInfo']]]],['blur',[['$forceUpdate']]]]}}" value="{{userInfo.phoneNum}}" bindinput="__e" bindblur="__e"/></view><view class="password input-box"><input class="input-text" type="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',['trim']],['userInfo']]]],['blur',[['$forceUpdate']]]]}}" value="{{userInfo.password}}" bindinput="__e" bindblur="__e"/></view><view class="section-box"><view class="remember"><label class="checkbox"><checkbox value="" checked="{{checked}}" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></checkbox></label><text>记住密码</text></view><view data-event-opts="{{[['tap',[['toForget',['$event']]]]]}}" class="forget" bindtap="__e">忘记密码</view></view><view class="btns"><view data-event-opts="{{[['tap',[['toRegister',['$event']]]]]}}" class="btn register-btn" bindtap="__e">注册</view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="btn login-btn" bindtap="__e">登录</view></view></view>