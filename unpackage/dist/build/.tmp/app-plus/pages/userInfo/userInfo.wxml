<view class="user-info" bind:__l="__l"><view data-event-opts="{{[['tap',[['changeAvatar',['$event']]]]]}}" class="change-avatar" bindtap="__e"><image class="avatar-url" src="{{userInfo.avatar||avatarUrl}}" mode></image><view class="avatar-btn">点击更换头像</view></view><view class="center-list"><view data-event-opts="{{[['tap',[['changeName',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="list-title">昵称</text><text class="list-text">{{userInfo.nickname}}</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['changeBirthday',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="list-title">生日</text><text class="list-text">{{userInfo.birthday}}</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['changeSign',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="list-title">签名</text><text class="list-text line-height-sm">{{userInfo.signature}}</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['changeSex',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="list-title">性别</text><text class="list-text">{{$root.f0}}</text><text class="icon2"></text></view></view><w-picker class="vue-ref" mode="date" startYear="1900" endYear="2015" defaultVal="{{value}}" themeColor="#f00" data-ref="picker" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e"></w-picker></view>