page {
  min-height: 100%;
}
.down-app {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  text-align: center;
  padding: 40rpx 0;
  height: 100%;
}
.down-app .down-tips {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border: 1px solid #f5f5f5;
  width: 100%;
  margin: 0 auto;
  font-size: 24rpx;
  padding: 10rpx 40rpx;
  color: #333;
  line-height: 1.5;
  text-align: left;
  margin-bottom: 60rpx;
  background: #eee;
}
.down-app .down-tips .tips-pic {
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  vertical-align: -2rpx;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 26rpx 26rpx;
  margin-right: 10rpx;
}
.down-app .content-box {
  width: 262rpx;
  margin: 0 auto;
}
.down-app .content-box .logo-pic {
  display: block;
  width: 262rpx;
  height: 262rpx;
}
.down-app .content-box .app-name {
  display: block;
  font-size: 36rpx;
  color: #393E4C;
  font-weight: bold;
  margin-top: 20rpx;
}
.down-app .content-box .app-version {
  display: block;
  font-size: 28rpx;
  color: #D1D3DE;
}
.down-app .down-btn,
.down-app .down-hover {
  display: block;
  width: 686rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 36rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  background: #00C8FD;
  color: #fff;
  border: 0 !impotant;
  outline: none !impotant;
  border-radius: 8rpx;
}

