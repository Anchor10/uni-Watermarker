<view class="register" bind:__l="__l"><view class="phone-num input-box"><input class="input-text" type="number" maxlength="11" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['$0','phoneNum','$event',['trim']],['userInfo']]]],['blur',[['$forceUpdate']]]]}}" value="{{userInfo.phoneNum}}" bindinput="__e" bindblur="__e"/></view><view class="code-num input-box"><input class="input-text" type="number" maxlength="4" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['$0','smscode','$event',['trim']],['']]]],['blur',[['$forceUpdate']]]]}}" value="{{smscode}}" bindinput="__e" bindblur="__e"/><view data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" class="{{['get-code',codeBtnShow?'':'colorGrey']}}" bindtap="__e">{{''+(codeBtnShow?'获取验证码':'重新发送('+count+'秒)')+''}}</view></view><view class="password input-box"><input class="input-text" type="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',['trim']],['userInfo']]]],['blur',[['$forceUpdate']]]]}}" value="{{userInfo.password}}" bindinput="__e" bindblur="__e"/></view><view class="invite-code input-box"><input class="input-text" type="text" placeholder="请输入邀请码（选填）" data-event-opts="{{[['input',[['__set_model',['$0','inviteCode','$event',['trim']],['']]]],['blur',[['$forceUpdate']]]]}}" value="{{inviteCode}}" bindinput="__e" bindblur="__e"/></view><view class="agreement"><label class="checkbox"><checkbox value="" checked="{{checked}}" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></checkbox></label><view class="agreement-text">我同意<text data-event-opts="{{[['tap',[['agreementShow',['$event']]]]]}}" bindtap="__e">《用户协议》</text></view></view><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="register-btn" bindtap="__e">立即注册</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" hidden="{{!(agreeShow)}}" class="agreement-bg" bindtap="__e"><view class="agreement-box"><view class="agreement-title">{{agreement.title}}</view><view class="agreement-content"><rich-text name="view" nodes="{{agreement.content}}"></rich-text></view><view class="close"></view></view></view></view>