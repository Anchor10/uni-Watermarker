  height: 64rpx;
  line-height: 64rpx;
  position: absolute;
  right: 0rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
          transform: translateY(-50%);
  color: #00C8FD;
  font-size: 28rpx;
  z-index: 1000;
}
