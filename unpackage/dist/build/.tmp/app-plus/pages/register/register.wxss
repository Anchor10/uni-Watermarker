  display: block;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  top: 0;
  background-image: url(data:image/png;base64,);
  background-position: center center;
  background-size: 32rpx 32rpx;
  background-color: #fff;
  border: 0;
}
.register .agreement .checkbox checkbox .wx-checkbox-input-checked::before {
