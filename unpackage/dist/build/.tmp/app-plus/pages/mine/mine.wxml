<view class="center" bind:__l="__l"><view class="blue-top"><view class="user-card"><view class="card-top"><view data-event-opts="{{[['tap',[['toUserInfo',['$event']]]]]}}" class="user-top" bindtap="__e"><image class="user-pic" src="{{userInfo.avatar||avatarUrl}}" mode></image><view class="user-board"><text class="user-name">{{login?userInfo.nickname:'点击登录'}}</text><block wx:if="{{login}}"><image class="vip-icon" src="{{vipIcon}}" mode></image></block></view></view></view><view class="card-bottom"><view class="left"><view class="count"><text class="num">{{userInfo.parse_times!=null?userInfo.parse_times:'-'}}</text></view><text class="txt">今日剩余解析次数</text></view><view data-event-opts="{{[['tap',[['canRecharge',['$event']]]]]}}" class="right" bindtap="__e"><view class="count"><text class="num">{{endDate}}</text></view><text class="txt">{{login&&userInfo.vip_end_time==null?'专享无限解析次数':'会员到期时间'}}</text></view></view></view></view><view class="center-list"><view data-event-opts="{{[['tap',[['toRecharge',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="icon1 recharge-icon"></text><text class="list-text">会员中心</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['toInvite',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="icon1 invite-icon"></text><text class="list-text">邀请有礼</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['toSecurity',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="icon1 security-icon"></text><text class="list-text">账号安全</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['toFeedback',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="icon1 feedback-icon"></text><text class="list-text">意见反馈</text><text class="icon2"></text></view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="center-list-item" bindtap="__e"><text class="icon1 setting-icon"></text><text class="list-text">设置</text><text class="icon2"></text></view></view></view>